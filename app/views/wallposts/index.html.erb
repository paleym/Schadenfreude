<h1>Listing wallposts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wallposts.each do |wallpost| %>
  <tr>
    <td><%= wallpost.content %></td>
    <td><%= wallpost.user_id %></td>
    <td><%= link_to 'Show', wallpost %></td>
    <td><%= link_to 'Edit', edit_wallpost_path(wallpost) %></td>
    <td><%= link_to 'Destroy', wallpost, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wallpost', new_wallpost_path %>
