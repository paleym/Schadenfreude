
%table
  %thead
    %tr
      %th First Name
      %th Last Name
      %th Birthday
      %th Email
  %tbody
    - @users.each do |user|
      %tr
        %td= user.firstName 
        %td= user.lastName
        %td= user.birthday
        %td= user.email
=params
%br
= link_to 'Add New User', new_user_path
